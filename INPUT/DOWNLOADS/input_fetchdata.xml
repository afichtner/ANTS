<?xml version='1.0' encoding='UTF-8'?>

<data_download>

    <!--verbose-->
    <verbose>1</verbose>
    
    
    <!--user identity-->
    <user>laura.ermert@derdw.ethz.ch</user>

    <!--networks, channels, stations, locations, etc.-->
    <data>
        <!--list of networks (separated by blanks) from which data are requested, can be virtual networks-->
        <networks>G</networks>
        <!--list of channels, separated by blanks-->
        <channels>BHE BHN BHZ</channels>
        <!-- list of locations, separated by blanks-->
        <locations>*</locations>
        <!--Download stations specified in this file. Set to * to download all stations.-->
        <stations>./INPUT/STATION_LISTS/staG_short.txt</stations>
    </data>

    <!--time window where data are requested-->
    <!--Format: YYYY-MM-DD,HH:MM:SS-->
    <time>
        <starttime>2012-01-01,00:00:00</starttime>
        <endtime>2012-02-01,00:00:00</endtime>
    </time>

    <!--geographic area where stations are considered-->
    <region>
        <lat_min>-90.0</lat_min>
        <lat_max>90.0</lat_max>
        <lon_min>-180.0</lon_min>
        <lon_max>180.0</lon_max>
    </region>

    <!--saving data-->
    <storage>
        <downloadloc>./DATA/G.2012.01.data</downloadloc>
        <respfileloc>./DATA/G.2012.01.resp</respfileloc>
    </storage>
    
</data_download>
