<?xml version='1.0' encoding='UTF-8'?>

<dataset_parameters>
    <!--datacentre-->
    <centre>iris</centre>
    <user>laura.ermert@derdw.ethz.ch</user>


    <!--network-->
    <network>G</network>
    <!--You either have to provide a station list in a separate file or use all stations of the network-->
    <stations>download/staG.txt</stations>
    <channel>BH*</channel>
    <location>*</location>
    
    <!--downsampling-->
    <rundown>1</rundown>
    <downs>10</downs>
    <!--Give the desired new sampling rate in Hz-->
    
    <!--geographic area-->
    <lat_min>-90.0</lat_min>
    <lat_max>90.0</lat_max>
    <lon_min>-180.0</lon_min>
    <lon_max>180.0</lon_max>
    
    <!--time-->
    <starttime>2013-01-01</starttime>
    <endtime>2013-01-02</endtime>
   
   <!--saving data-->
   <format>mseed</format>
   <logfilename>avp_testlog.txt</logfilename>
   <downloadloc>./testdata</downloadloc>
   <targetdir>./testdata_G</targetdir>
   
    
</dataset_parameters>
